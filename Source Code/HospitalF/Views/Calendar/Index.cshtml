@using HospitalF.Constant;
@{
    ViewBag.Title = "Quản lý lịch hẹn";
}
@{
    string username = string.Empty;
    string email = string.Empty;

    if (HospitalF.Models.SimpleSessionPersister.Username != null)
    {
        string[] userInfo = HospitalF.Models.SimpleSessionPersister.Username.
           Split(Char.Parse(Constants.Minus));
        username = userInfo[1];
        email = userInfo[0];
    }
    string[] strEmail=email.Split('@');
    string encodeEmail = strEmail[0] + "%40" + strEmail[1];
}
<iframe src="https://www.google.com/calendar/embed?showTitle=0&amp;showPrint=0&amp;mode=WEEK&amp;height=600&amp;wkst=2&amp;hl=vi&amp;bgcolor=%23ffffff&amp;src=@encodeEmail&amp;color=%23691426&amp;ctz=Asia%2FSaigon" style="border-width: 0" width="100%" height="600" frameborder="0" scrolling="no"></iframe>
